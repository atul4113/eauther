<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel ui:field="panel" styleName="mainPage">
		<div class="mainPageHeader">
			<h3 ui:field="title"></h3>
			<a class="mainPageCloseBtn closeWidgetLocker"></a>
		</div>
		
		<g:HTMLPanel ui:field="contents" styleName="contents">
			<div class="propertiesItemsListWrapper" ui:field="wrapper">
				<g:HTMLPanel ui:field="list" styleName="propertiesItemsList">
				</g:HTMLPanel>
			</div>
			
			<div class="propertiesControls">
				<span></span>
				<input class="noOfItems" type="text" value="1" ui:field="addItemsCount"/>
				<a class="button" ui:field="add"></a>
				<a class="button" ui:field="addAll"></a>
				<a class="button" ui:field="sort"></a>
				
				<div class="saveButtons">
					<a class="button" ui:field="load"></a>
					<a class="button" ui:field="apply"></a>
					<a class="button" ui:field="save"></a>
				</div>
			</div>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 