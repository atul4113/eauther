<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel styleName="stylesWidget" ui:field="stylesWidget">
		<div class="boxName"></div>
		<div class="boxContents cssClass">
			<div class="property-cssClass">
				<div class="stylesLabel"></div>
				<div class="stylesValue">
					<select name="CSSClass" ui:field="cssClass">
					</select>
				</div>
			</div>
			
			<div class="propertiesSeparator"></div>
			
			<div class="property-cssClass">
				<div class="stylesLabel"></div>
				<div class="stylesValue">
					<textarea ui:field="inlineCSS"></textarea>
				</div>
				<div id="updateStylesButton">
					<a class="button" ui:field="updateStyles"></a>
				</div>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 