<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	</ui:style>
	
	<g:HTMLPanel styleName="modal-box prompt" ui:field="panel">
		<span ui:field="title"></span>
		<br/>
		<br/>
		<span ui:field="textBefore"></span>
		<input type="text" ui:field="styleName"></input>
		<div class="add-new-style-modal-row add-new-style-modal-action-controls">
			<a class="accept" ui:field="addButton"></a>
			<a class="decline" ui:field="declineButton"></a>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 