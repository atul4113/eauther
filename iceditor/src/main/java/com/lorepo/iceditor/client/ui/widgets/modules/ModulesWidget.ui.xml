<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel ui:field="panel" styleName="widget-box">
		<div class="boxName">
			<span ui:field="title"></span>
			<a class="expandBtn expandBtn-modules expanded"></a>
		</div>
		<div class="boxContents boxContents-modules">
			<div id="modulesMenuPanel" class="buttonsPanel">
				<a ui:field="showModules" class="menuButton tooltip-bottom" data-tooltip="" ></a>
				<a ui:field="hideModules" class="menuButton tooltip-bottom" data-tooltip=""></a>
				<a ui:field="lockModules" class="menuButton tooltip-bottom" data-tooltip=""></a>
				<a ui:field="unlockModules" class="menuButton tooltip-bottom" data-tooltip=""></a>
				<a ui:field="moduleUp" class="menuButton tooltip-bottom" data-tooltip=""></a>
				<a ui:field="moduleDown" class="menuButton tooltip-bottom" data-tooltip=""></a>
				<a ui:field="bringToFront" class="menuButton tooltip-bottom" data-tooltip=""></a>
				<a ui:field="sendBack" class="menuButton tooltip-bottom" data-tooltip=""></a>
				<a ui:field="sortModules" class="menuButton tooltip-bottom" data-tooltip=""></a>
			</div>
			<g:HTMLPanel ui:field="modulesList" styleName="boxList"></g:HTMLPanel>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 