<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel ui:field="panel" styleName="mainPage">
		<div class="mainPageHeader">
			<h3></h3>
			<a ui:field="closeBtn" class="mainPageCloseBtn closeWidgetLocker"></a>
		</div>

		<g:HTMLPanel styleName="contents scrollable" ui:field="contents"></g:HTMLPanel>

		<div class="textEditorControls">
			<a ui:field="apply" class="controlsBtn"></a>
			<a ui:field="save" class="controlsBtn"></a>
		</div>	
	</g:HTMLPanel>
</ui:UiBinder> 