<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel styleName="modal-box prompt modal_filledgap_add" ui:field="panel">
		<span></span>
		<br/>
		<div class="addGapsTitle">
			<span ui:field="title"></span><br/>
		</div>
		<div class="filled-gap-initial-label">
			<span ui:field="initialTextLabel"></span> <input type="text" ui:field="initialText"></input><br/>
		</div>
		<span ui:field="answers"></span>
		<g:HTMLPanel ui:field="items">
		</g:HTMLPanel>
		<a ui:field="add" class="add"></a>
		<div class="confirmationPanel">
			<span ui:field="feedback"></span><br/>
			<a class="accept"></a>
			<a class="decline"></a>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 