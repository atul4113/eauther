<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:semi-responsive="urn:import:com.lorepo.iceditor.client.semi.responsive.ui.widgets">
	
	<ui:style>
	
	</ui:style>

	<g:HTMLPanel styleName="modal-box prompt" ui:field="panel">
		<span ui:field="title"></span>
		<br />
		<br />
		<span></span>
		<div class="semi-responsive-layout-modal-form-container">
			<semi-responsive:SemiResponsiveCSSListWidget ui:field="stylesList"></semi-responsive:SemiResponsiveCSSListWidget>
		</div>
		<div
			class="add-new-semi-responsive-layout-modal-row add-new-semi-responsive-layout-modal-action-controls">
			<a class="accept" ui:field="acceptButton"></a>
			<a class="decline" ui:field="declineButton"></a>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 