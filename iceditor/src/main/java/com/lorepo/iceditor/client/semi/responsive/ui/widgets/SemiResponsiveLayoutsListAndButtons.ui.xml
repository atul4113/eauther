<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:r="urn:import:com.lorepo.iceditor.client.semi.responsive.ui.widgets">
	<ui:style>
	</ui:style>
	<g:HTMLPanel styleName="semi-responsive-layouts-editing-form">
		<div class="semi-responsive-layouts-editing-form-list">
			<r:SemiResponsiveLayoutsListWidget 
			visibleItemCount="10"
			emptyChangeHandler="true"
			ui:field="semiResponsiveList"></r:SemiResponsiveLayoutsListWidget>
		</div>
		<div class="semi-responsive-layouts-editing-form-container">
			<div class="semi-responsive-layouts-editing-form-actions">
				<a
					class="semi-responsive-layouts-editing-form-action-button semi-responsive-layouts-editing-form-action-add-new-layout tooltip-bottom"
					data-tooltip="Add new semi responsive layout"
					ui:field="addSemiResponsiveLayout"></a>
				<a
					class="semi-responsive-layouts-editing-form-action-button semi-responsive-layouts-editing-form-action-delete-layout tooltip-bottom"
					data-tooltip="Delete semi responsive layout"
					ui:field="deleteSemiResponsiveLayout"></a>
				<a
					class="semi-responsive-layouts-editing-form-action-button semi-responsive-layouts-editing-form-action-set-as-default tooltip-bottom"
					data-tooltip="Set as default semi responsive layout"
					ui:field="setAsDefaultSemiResponsiveLayout"></a>
				<a
					class="semi-responsive-layouts-editing-form-action-button semi-responsive-layouts-editing-form-action-set-semi-responsive-layout-style tooltip-bottom"
					data-tooltip="Set as semi responsive layout css style"
					ui:field="setSemiResponsiveLayoutCssStyle"></a>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 