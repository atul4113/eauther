<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:semi-responsive="urn:import:com.lorepo.iceditor.client.semi.responsive.ui.widgets">
	<ui:style>
	</ui:style>
	
	
	<g:HTMLPanel styleName="semi-responsive-layouts-editing-form">
		<div class="semi-responsive-layouts-editing-form-list">
			<semi-responsive:SemiResponsiveCSSListWidget
			ui:field="semiResponsiveCSSListWidget"
			visibleItemCount="10"
			emptyChangeHandler="true"></semi-responsive:SemiResponsiveCSSListWidget>
		</div>
		<div class="semi-responsive-layouts-editing-form-container">
			<div class="semi-responsive-layouts-editing-form-actions">
				<a
					class="semi-responsive-layouts-editing-form-action-button semi-responsive-layouts-editing-form-action-add-new-css-style tooltip-bottom"
					data-tooltip="Add new semi responsive css style"
					ui:field="addSemiResponsiveCSSStyle"></a>
				<a
					class="semi-responsive-layouts-editing-form-action-button semi-responsive-layouts-editing-form-action-delete-css-style tooltip-bottom"
					data-tooltip="Delete semi responsive css style"
					ui:field="deleteSemiResponsiveCSSStyle"></a>
				<a
					class="semi-responsive-layouts-editing-form-action-button semi-responsive-layouts-editing-form-action-set-as-default-style tooltip-bottom"
					data-tooltip="Set as default semi responsive css style"
					ui:field="setAsDefaultSemiResponsiveCSSStyle"></a>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 