<?xml version='1.0' encoding='utf-8'?>
<module rename-to="iceditor">
	<!-- Inherit the core Web Toolkit stuff.                        -->
  	<inherits name="com.google.gwt.user.User"/>
  	<inherits name="com.google.gwt.i18n.I18N"/>
  	<inherits name="com.google.gwt.json.JSON"/>
  	
  	<!-- Inherit third-party libraries -->
  	<inherits name="com.google.gwt.query.Query"/>
  
  	<!-- Other module inherits                                      -->
  	<inherits name="com.lorepo.icf.Widgets"/>
  	<inherits name="com.lorepo.icf.Scripting"/>
  	<inherits name="com.lorepo.icf.UIDesigner"/>
  	<inherits name="com.lorepo.icf.Undo"/>
  	<inherits name="com.lorepo.icplayer.Icplayer"/>

	<script src="codemirror/codemirror-compressed.js"/>
	<script src="codemirror/searchcursor.js"/>
	<script src="codemirror/search.js"/>
	<script src="codemirror/dialog.js"/>

	<stylesheet src="codemirror/codemirror.css"/>
	<stylesheet src="codemirror/dialog.css"/>

	<stylesheet src="theme/perfect-scrollbar.css"/>
	
	<!--  Editor theme -->
	<stylesheet src="theme/editor.css"/>
	<stylesheet src="theme/screen.css"/>
	<stylesheet src="theme/tooltips.css"/>
	<stylesheet src="theme/jquery.colorpicker.css"/>
	<stylesheet src="theme/jquery-ui.theme.min.css"/>
	<stylesheet src="theme/colorpicker.css"/>
	<stylesheet src="libs/jquery-ui.css"/>

    <!-- LESS CSS COMPILATION BUNDLE -->
    <stylesheet src="dist/main.min.css"/>

	<!-- Third-party scripts -->
	<script src="scripts/perfect-scrollbar.js"/>
	<script src="scripts/Sortable.js"/>
	<script src="scripts/jquery.colorpicker.js"/>
	<script src="scripts/colorpicker.js"/>

	<script src="libs/jquery-ui.js"/>

	
	<!-- Editor scripts -->
	<script src="scripts/resizeReciveMessage.js"/>
	<script src="scripts/jQueryManager.js"/>
	<script src="scripts/pageEnterSizesTimeout.js"/>
	<script src="scripts/PropertyValueHandler.js"/>
	<script src="scripts/main.js"/>

  	<!-- Specify the app entry point class.                         -->
  	<entry-point class="com.lorepo.iceditor.client.EditorEntryPoint"/>

  	<!-- Specify the paths for translatable code                    -->
  	<source path="client"/>
	<set-property name="user.agent" value="safari,ie9,gecko1_8, opera"/>
</module>